RM			=	rm -f
ECHO		=	echo

all			:
				@$(MAKE) $(NAME)
				@$(ECHO) "*** Make <symbol>."

$(NAME)		:
				@$(ECHO) $(SYMBOL) > $(NAME)

clean		:
				@$(RM) $(NAME)
				@$(ECHO) "*** Clean <symbol>."

fclean		:
				@$(RM) $(NAME)
				@$(ECHO) "*** Fclean <symbol>."

re			:
				@$(MAKE) fclean
				@$(MAKE) all

.PHONY		:	all clean fclean re
